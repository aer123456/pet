/* template/perferList */
.title-wrap {
  display: flex;
  justify-content: space-between;
  align-items: center;
  margin: 0 40rpx;
}
.title-wrap .title {
  font-size: 40rpx;
  line-height: 60rpx;
  color: #333;
}
.title-wrap .title text:first-child {
  margin-right: 20rpx;
  font-weight: bold;
}
.title-wrap .more-link {
  width: 46rpx;
  height: 46rpx;
}
.title-wrap .more-link-icon {
  width: 46rpx;
  height: 46rpx;
  vertical-align: middle;
}
.title-wrap .more-link-hover {
  background-color: transparent;
  opacity: 0.8;
}
.icon-size {
  width: 46rpx;
  height: 46rpx;
}
