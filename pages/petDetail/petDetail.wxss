.pet-detail-page {
  padding-bottom: 110rpx;
  background: #000;
}
.pet-detail-page .pet-detail-banner {
  width: 100%;
  height: 500rpx;
  position: relative;
}
.pet-detail-page .pet-detail-banner swiper {
  width: 100%;
  height: 100%;
  text-align: center;
}
.pet-detail-page .pet-detail-banner swiper image,
.pet-detail-page .pet-detail-banner swiper video {
  width: 100%!important;
  height: 100%!important;
}
.pet-detail-page .pet-detail-banner .banner-counts {
  position: absolute;
  right: 40rpx;
  bottom: 60rpx;
  height: 72rpx;
  line-height: 72rpx;
  border-radius: 36rpx;
  padding: 0 30rpx;
  color: #ffffff;
  background: rgba(0, 0, 0, 0.1);
  text-align: center;
}
.pet-detail-page .pet-detail-content-wrap {
  background-color: #fff;
  border-radius: 40rpx 40rpx 0px 0px;
  margin-top: -20rpx;
  z-index: 999;
  position: relative;
}
.pet-detail-page .pet-detail-content-wrap .view1 {
  padding: 30rpx 60rpx;
}
.pet-detail-page .pet-detail-content-wrap .view1 .title {
  font-size: 46rpx;
  color: #424242;
}
.pet-detail-page .pet-detail-content-wrap .view1 .title image {
  width: 32rpx;
  height: 32rpx;
  vertical-align: middle;
  margin-left: 14rpx;
}
.pet-detail-page .pet-detail-content-wrap .view1 .birth {
  font-size: 32rpx;
  color: #424242;
  margin: 10rpx 0;
}
.pet-detail-page .pet-detail-content-wrap .view1 .birth image {
  width: 30rpx;
  height: 30rpx;
  margin-right: 14rpx;
}
.pet-detail-page .pet-detail-content-wrap .view1 .other {
  font-size: 30rpx;
  color: #b8bbbc;
}
.pet-detail-page .pet-detail-content-wrap .view1 .marks {
  margin: 10rpx 0;
}
.pet-detail-page .pet-detail-content-wrap .view1 .marks text {
  display: inline-block;
  background: #fef2e3;
  border-radius: 12rpx 12rpx 12rpx 0px;
  font-size: 28rpx;
  color: #f0af5a;
  padding: 5rpx 10rpx;
  margin-right: 12rpx;
}
.pet-detail-page .pet-detail-content-wrap .view1 .price {
  font-size: 46rpx;
  color: #f0af5a;
}
.pet-detail-page .pet-detail-content-wrap .view1 .price .discount {
  font-size: 28rpx;
  color: #b8bbbc;
  text-decoration: line-through;
}
.pet-detail-page .pet-detail-content-wrap .activity {
  width: 100%;
  height: 109rpx;
  line-height: 109rpx;
  background: #fef2e3;
  padding-left: 60rpx;
  color: rgba(240, 175, 90, 0.6);
  font-size: 30rpx;
}
.pet-detail-page .pet-detail-content-wrap .activity text {
  display: inline-block;
  color: #fff;
  background: rgba(253, 198, 127, 0.8);
  height: 45rpx;
  line-height: 45rpx;
  padding: 5rpx 8rpx;
}
.pet-detail-page .pet-detail-content-wrap .activity .left {
  border-radius: 12rpx 0 0 12rpx;
  margin-left: 24rpx;
  margin-right: 3rpx;
}
.pet-detail-page .pet-detail-content-wrap .activity .middle {
  margin: 0 3rpx;
}
.pet-detail-page .pet-detail-content-wrap .activity .right {
  border-radius: 0 12rpx 12rpx 0;
  margin-left: 3rpx;
}
.pet-detail-page .pet-detail-content-wrap .address {
  height: 90rpx;
  line-height: 90rpx;
  font-size: 28rpx;
  color: #424242;
  padding: 0 60rpx;
  border-bottom: 1rpx solid #F4F5F7;
}
.pet-detail-page .pet-detail-content-wrap .address image {
  width: 30rpx;
  height: 30rpx;
  margin-right: 14rpx;
}
.pet-detail-page .pet-detail-content-wrap .tabs {
  height: 140rpx;
  padding: 0 60rpx;
  display: flex;
  align-items: center;
  justify-content: space-around;
  border-bottom: 1rpx solid #F4F5F7;
}
.pet-detail-page .pet-detail-content-wrap .tabs .tab {
  border-radius: 10rpx;
  opacity: 0.5;
  border: 2rpx solid #b8bbbc;
  font-size: 30rpx;
  color: #b8bbbc;
  padding: 10rpx;
}
.pet-detail-page .pet-detail-content-wrap .tabs .current {
  border: 2rpx solid #facc90;
  color: #fdc67f;
}
.pet-detail-page .pet-detail-content-wrap .tab1-content {
  padding: 0 0 30rpx 0;
}
.pet-detail-page .pet-detail-content-wrap .tab1-content .padding-60 {
  padding-left: 60rpx;
  padding-right: 60rpx;
}
.pet-detail-page .pet-detail-content-wrap .tab1-content .title {
  font-size: 36rpx;
  color: #424242;
  margin: 60rpx 0 30rpx;
}
.pet-detail-page .pet-detail-content-wrap .tab1-content .title text {
  display: inline-block;
  font-weight: bold;
}
.pet-detail-page .pet-detail-content-wrap .tab1-content .title .right {
  font-size: 28rpx;
  font-weight: 400;
  color: #424242;
  line-height: 40rpx;
  float: right;
  padding-top: 7rpx;
}
.pet-detail-page .pet-detail-content-wrap .tab1-content .color-wrap {
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  font-size: 24rpx;
  color: #c6c9c9;
  text-align: center;
  margin-bottom: 20rpx;
}
.pet-detail-page .pet-detail-content-wrap .tab1-content .color-wrap .color-content {
  flex: 1;
  margin-bottom: 15rpx;
  height: 110rpx;
}
.pet-detail-page .pet-detail-content-wrap .tab1-content .color-wrap .color-content .img {
  position: relative;
}
.pet-detail-page .pet-detail-content-wrap .tab1-content .color-wrap .color-content .img .ig {
  width: 60rpx;
  height: 50rpx;
  padding-top: 12rpx;
}
.pet-detail-page .pet-detail-content-wrap .tab1-content .color-wrap .color-content .img .bg {
  width: 95rpx;
  height: 74rpx;
  position: absolute;
  left: 11rpx;
  top: 0;
}
.pet-detail-page .pet-detail-content-wrap .tab1-content .color-wrap .color-content text {
  display: block;
  margin-top: 14rpx;
}
.pet-detail-page .pet-detail-content-wrap .tab1-content .eye-wrap .img .ig {
  width: 56rpx;
  height: 56rpx;
}
.pet-detail-page .pet-detail-content-wrap .tab1-content .eye-wrap .img .bg {
  width: 93rpx!important;
  height: 80rpx!important;
  left: 6rpx!important;
  top: -4rpx !important;
}
.pet-detail-page .pet-detail-content-wrap .tab1-content .tab-items {
  display: flex;
  flex-direction: row;
  align-items: center;
}
.pet-detail-page .pet-detail-content-wrap .tab1-content .tab-items .tab-item {
  position: relative;
  width: 103rpx;
  height: 48rpx;
  line-height: 48rpx;
  border-radius: 9rpx;
  border: 2rpx solid #dbdddd;
  font-size: 28rpx;
  color: #dbdddd;
  margin-right: 20rpx;
  text-align: center;
}
.pet-detail-page .pet-detail-content-wrap .tab1-content .tab-items .tab-item image {
  width: 28rpx;
  height: 28rpx;
  position: absolute;
  bottom: -10rpx;
  right: -10rpx;
}
.pet-detail-page .pet-detail-content-wrap .tab1-content .tab-items .current {
  color: #fdc67f;
  border: 2rpx solid #fdc67f;
}
.pet-detail-page .pet-detail-content-wrap .tab1-content .font-descs {
  font-size: 28rpx;
  color: #424242;
  line-height: 40rpx;
}
.pet-detail-page .pet-detail-content-wrap .tab1-content .font-descs .intro {
  display: block;
  margin-bottom: 24rpx;
}
.pet-detail-page .pet-detail-content-wrap .tab1-content .gap {
  width: 100%;
  height: 0;
  border-bottom: 2rpx solid #f4f5f7;
  padding-top: 30rpx;
}
.pet-detail-page .pay-buttons {
  height: 150rpx;
  padding: 0 60rpx;
  display: flex;
  justify-content: space-between;
  align-items: top;
  position: fixed;
  bottom: 0;
  left: 0;
  right: 0;
  background: #ffffff;
  z-index: 1000;
}
.pet-detail-page .pay-buttons .chat-wrap {
  width: 110rpx;
  height: 110rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  border-radius: 20rpx;
}
.pet-detail-page .pay-buttons .chat-wrap image {
  width: 53rpx;
  height: 53rpx;
}
.pet-detail-page .pay-buttons .pay-button {
  width: 480rpx;
  height: 110rpx;
  background: #fdc67f;
  border-radius: 20rpx;
  font-size: 36rpx;
  color: #ffffff;
  text-align: center;
  display: flex;
  justify-content: center;
  align-items: center;
}
.pet-detail-page .pay-buttons .pay-button image {
  width: 45rpx;
  height: 20rpx;
  margin-left: 25rpx;
}
.pet-detail-page .pay-modal {
  position: fixed;
  top: 0;
  bottom: 0;
  left: 0;
  right: 0;
  background: rgba(0, 0, 0, 0.65);
}
.pet-detail-page .pay-modal .pay-dingjin {
  height: 500rpx;
  position: absolute;
  bottom: 0;
  left: 0;
  right: 0;
  background: #ffffff;
  border-radius: 40rpx 40rpx 0 0;
  padding: 40rpx;
}
.pet-detail-page .pay-modal .pay-dingjin image {
  position: absolute;
  right: 40rpx;
  top: 40rpx;
  display: block;
  width: 42rpx;
  height: 42rpx;
}
.pet-detail-page .pay-modal .pay-dingjin .title {
  font-size: 40rpx;
  color: #333333;
  text-align: center;
  margin: 40rpx 0;
}
.pet-detail-page .pay-modal .pay-dingjin input {
  display: block;
  width: 100%;
  height: 90rpx;
  background: #f7f9fb;
  border-radius: 20rpx;
  padding: 0 40rpx;
  font-size: 32rpx;
  color: #333333;
}
.pet-detail-page .pay-modal .pay-dingjin input:-webkit-input-placeholder {
  color: #929495;
}
.pet-detail-page .pay-modal .pay-dingjin input:-moz-placeholder {
  color: #929495;
}
.pet-detail-page .pay-modal .pay-dingjin input::-moz-placeholder {
  color: #929495;
}
.pet-detail-page .pay-modal .pay-dingjin input:-ms-input-placeholder {
  color: #929495;
}
.pet-detail-page .pay-modal .pay-dingjin .pay-btn {
  height: 90rpx;
  line-height: 90rpx;
  background: #fdc67f;
  border-radius: 20rpx;
  text-align: center;
  color: #ffffff;
  margin-top: 60rpx;
}
.pet-detail-page .pay-modal .pay-dingjin .disable {
  opacity: 0.4;
}
