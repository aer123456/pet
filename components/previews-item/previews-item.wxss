.previews-link {
  display: flex;
  width: 100%;
  height: 100%;
}
.previews-item {
  display: flex;
  align-items: center;
  padding-left: 20rpx;
  width: 100%;
}
.previews-thumbnail {
  width: 120rpx;
  height: 120rpx;
  border-radius: 50%;
  vertical-align: middle;
}
.previews-details {
  margin-left: 14rpx;
  font-size: 32rpx;
  line-height: 45rpx;
  color: #424242;
}
